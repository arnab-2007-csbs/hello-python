a =int(input("enter your first number"))
b =int(input("enter your second number "))
c =input("choose anyone between div, sub, mul, add")
if c == "mul" :
    print("answer=", a*b)
elif  c == "div" :
    print("answer=", a/b)
elif c == "sub" :
    print("answer=", a-b)
elif c == "add":
    print("answer=", a+b)
else : 
    print("please choose between add, sub, mul, div")